    "import numpy as np\n",
    "\n",
    "X = np.array([\n",
    "    [1, 1],\n",
    "    [1, 2],\n",
    "    [1, 3],\n",
    "    [1, 4]\n",
    "])\n",
    "\n",
    "Y = np.array([2, 3, 3, 6])\n",
    "\n",
    "XtX = np.array([\n",
    "    [2, 3, 4, 5],\n",
    "    [3, 5, 7, 9],\n",
    "    [4, 7, 10, 13],\n",
    "    [5, 9, 13, 17]\n",
    "])\n",
    "\n",
    "XtXinv = np.linalg.inv(X.T @ X)\n",
    "b = XtXinv @ X.T @ Y\n",
    "b\n",
    "X.T @ X"
   ]
  }
